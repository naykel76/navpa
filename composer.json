{
    "name": "naykel/navpa",
    "description": "Navigation and Page Management Package for NayKel Laravel Applications",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "homepage": "https://github.com/naykel76/navpa.git",
    "keywords": ["Laravel", "Navpa"],
    "require": {
        "illuminate/support": "~7|~8",
        "naykel/formit": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Navpa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Navpa\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Navpa\\NavpaServiceProvider"
            ],
            "aliases": {
                "Navpa": "Naykel\\Navpa\\Facades\\Navpa"
            }
        }
    }
}
